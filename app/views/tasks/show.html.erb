<div class="container">
  <h1 class='card-title'><%= @task.title %></h1>
  <h5 class='card card-body'><%= @task.details %></h5>
  <div class="card-body">
    <p>Task ID: <%= params[:id] %></p>
    <p>completed: <%= @task.completed %></p>
    <p>created_at: <%= @task.created_at %></p>
    <p>updated_at: <%= @task.updated_at %></p>
  </div>
</div>
